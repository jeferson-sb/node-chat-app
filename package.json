{
  "name": "chatme",
  "version": "1.0.0",
  "description": "Chat app built with Express & Socket.io",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node src/presentation/server.js",
    "concurrently": "concurrently \"npm run dev:server\" \"npm run dev:client\" ",
    "dev:server": "nodemon src/presentation/server.js",
    "dev:client": "npm run dev --prefix client",
    "build:client": "npm run build --prefix client",
    "lint": "eslint . --ext .js",
    "prettier": "prettier --write \"**/*.{js,json}\""
  },
  "keywords": [],
  "author": "Jeferson S. Brito",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.0",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0"
  }
}
