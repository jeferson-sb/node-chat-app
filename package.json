{
  "name": "chatme",
  "version": "1.0.0",
  "description": "Chat app built with Express & Socket.io",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node src/presentation/server.js",
    "concurrently": "concurrently \"npm run dev:server\" \"npm run dev:client\" ",
    "dev:server": "node --watch src/presentation/server.js",
    "dev:client": "npm run dev --prefix client",
    "build:client": "npm run build --prefix client",
    "lint": "eslint . --ext .js",
    "prettier": "prettier --write \"**/*.{js,json}\"",
    "upgrade": "taze -I"
  },
  "keywords": [],
  "author": "Jeferson S. Brito",
  "license": "MIT",
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "concurrently": "^9.1.2",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "morgan": "^1.10.1",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.1",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "taze": "^19.1.0"
  }
}
